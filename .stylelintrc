{
	"extends": "stylelint-config-recommended-less",
	"rules": {
		"block-no-empty": null,
		"rule-empty-line-before": [
			"always", {
				"except": ["first-nested"],
				"ignore": ["after-comment"],
				"severity": "warning"
			}
		],
		"no-descending-specificity": null,
		"no-duplicate-selectors": null
	}
}
